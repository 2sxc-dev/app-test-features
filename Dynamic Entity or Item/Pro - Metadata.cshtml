@inherits Custom.Hybrid.RazorTyped
@using System.Linq;

<h1>Pro - Stack and Keys</h1>
<p>
  Content-Type used: <code>WithMetadata</code>
</p>
@{
  var item = AsItem(App.Data["WithMetadata"].First());
  var md = item.Metadata;
}
<ol>
  <li>Md.Title: @md.Title</li>
  <li>Md.Get Age: @md.Get("Age")</li>
  <li>Md.Get PropOnMainItem: @md.Get("PropOnMainItem")</li>
</ol>