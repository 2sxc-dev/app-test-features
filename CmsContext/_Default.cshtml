@inherits Custom.Hybrid.Razor14

<h1>CmsContext</h1>

@{
  var view = CmsContext.View;
}

<h2>View</h2>
<ol>
  <li>
    Id: @view.Id
  </li>
  <li>
    Name: @view.Name
  </li>
  <li>
    Identifier: @view.Identifier
  </li>
  <li>
    Edition: @view.Edition
  </li>
  <li>
    Metadata: @view.Metadata
  </li>
  <li>
    Path: @view.Path
  </li>
  <li>
    Path Shared: @view.PathShared
  </li>
  <li>
    PhysicalPath: @view.PhysicalPath
  </li>
  <li>
    PhysicalPath: @view.PhysicalPathShared
  </li>
</ol>